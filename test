_G.autofarm_configuration = {
    enabled = false,
    key_bind = "[",
    auto_punch = true,
    target_names = {
        [1] = "Saiyan",
        [2] = "Saiba",
        [3] = "Kick",
        [4] = "Chi",
        [5] = "Majin"
    }
}
if not game:IsLoaded() then game.Loaded:Wait() end
local url = "https://raw.githubusercontent.com/PrathamWasTaken/Promasu/main/Autofarm.lua"
loadstring(game:HttpGet(url))()
